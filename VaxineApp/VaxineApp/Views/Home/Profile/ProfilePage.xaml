<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VaxineApp.Views.Home.Profile.ProfilePage"
             xmlns:local="clr-namespace:VaxineApp.Renderers"
             xmlns:viewmodel="clr-namespace:VaxineApp.ViewModels"
             Title="Profile">
  <ContentPage.BindingContext>
    <viewmodel:ProfileViewModel></viewmodel:ProfileViewModel>
  </ContentPage.BindingContext>
  <ContentPage.ToolbarItems>
    <ToolbarItem IconImageSource="Universal_Edit.png"
                 Command="{Binding EditProfileCommand}" />
  </ContentPage.ToolbarItems>
    <ContentPage.Content>
    <Label TextColor="Black" Text="Hello"></Label>
    </ContentPage.Content>
</ContentPage>